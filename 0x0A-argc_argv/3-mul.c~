#include <stdio.h>
#include <stlib.h>

/**
 * main - print product of two  arguments
 * @argc: arguments count
 * @argv: argument vector
 *
 * Return: 0 sucess and 1 Error
 */

int main(int argc, char *argv[])
{
	if (argc == 3)
	{
		printf("%d\n", atoi(argv[1]) * atoi(argv[2]));
	}
	else
	{
		printf("Error\n");
		return (1);
	}

	return (0);
}
